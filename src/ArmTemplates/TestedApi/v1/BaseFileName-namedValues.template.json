{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "apimServiceName": {
      "type": "string"
    }
  },
  "resources": [
    {
      "apiVersion": "2021-08-01",
      "type": "Microsoft.ApiManagement/service/namedValues",
      "name": "[concat(parameters('apimServiceName'), '/default-rate-limit')]",
      "properties": {
        "tags": [],
        "secret": false,
        "displayName": "default-rate-limit",
        "value": "500"
      }
    },
    {
      "apiVersion": "2021-08-01",
      "type": "Microsoft.ApiManagement/service/namedValues",
      "name": "[concat(parameters('apimServiceName'), '/default-renewal-period')]",
      "properties": {
        "tags": [],
        "secret": false,
        "displayName": "default-renewal-period",
        "value": "60"
      }
    }
  ]
}